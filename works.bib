@article{CALD-DAMA,
	Abstract = {Objectives This study quantifies the prevalence and rates of discharge against medical advice (DAMA) in culturally and linguistically diverse (CALD) children and assesses the independent association between CALD status and DAMA accounting for key demographic confounders in a large tertiary Australian hospital network.Methods Prospectively collected data between 2010 and 2018 were extracted from the hospital network electronic medical records system for admitted patients (n=192 037), outpatients (n=268 904) and between 2015 and 2018 for emergency department (ED) patients (n=158 903). CALD status was defined as {\textquoteleft}preferred language being not English{\textquoteright} and DAMA was measured as {\textquoteleft}discharge at own risk{\textquoteright} in admissions, {\textquoteleft}no show{\textquoteright} in outpatients, {\textquoteleft}left without being seen{\textquoteright} and {\textquoteleft}did not wait{\textquoteright} in ED. Data were analysed using χ{\texttwosuperior} test, bivariate analysis and multivariate logistic regression.Results The prevalence of DAMA was 1.34\% in admitted patients, 19.31\% in outpatients and 12.64\% in ED patients. Rates of DAMA were higher among CALD children compared to non-CALD children (1.75\% vs 1.29\% in admitted patients, 26.53\% vs 17.92\% in outpatients and 18.74\% vs 11.61\% in ED patients). CALD status was independently associated with DAMA in admitted children (OR=1.30, 95\% CI 1.15 to 1.44), outpatients (OR=1.55; 95\% CI 1.51 to 1.58) and ED patients (OR=1.60; 95\% CI 1.53 to 1.66).Conclusion Being from a CALD background places children at increased risks to DAMA. Implementing appropriate health service responses may ensure equitable access and quality care for children from CALD backgrounds to reduce the rates of DAMA and its associated ramifications.},
	Author = {Guo, Xin Yue and Woolfenden, Susan and McDonald, Gordon and Saavedra, Aldo and Lingam, Raghu},
	Date-Added = {2020-06-28 17:08:39 +1000},
	Date-Modified = {2020-06-28 17:08:46 +1000},
	Doi = {10.1136/archdischild-2019-317063},
	Eprint = {https://adc.bmj.com/content/104/12/1150.full.pdf},
	Issn = {0003-9888},
	Journal = {Archives of Disease in Childhood},
	Number = {12},
	Pages = {1150--1154},
	Publisher = {BMJ Publishing Group Ltd},
	Title = {Discharge against medical advice in culturally and linguistically diverse Australian children},
	Url = {https://adc.bmj.com/content/104/12/1150},
	Volume = {104},
	Year = {2019},
	Bdsk-Url-1 = {https://adc.bmj.com/content/104/12/1150},
	Bdsk-Url-2 = {https://doi.org/10.1136/archdischild-2019-317063}}

@article{fish,
	Abstract = { In the wild, prey species often live in the vicinity of predators, rendering the ability to assess risk on a moment-to-moment basis crucial to survival. Visual cues are important as they allow prey to assess predator species, size, proximity and behaviour. However, few studies have explicitly examined prey's ability to assess risk based on predator behaviour and orientation. Using mosquitofish, Gambusia holbrooki, and their predator, jade perch, Scortum barcoo, under controlled conditions, we provide some of the first fine-scale characterization of how prey adapt their behaviour according to their continuous assessment of risk based on both predator behaviour and angular distance to the predator's mouth. When these predators were inactive and posed less of an immediate threat, prey within the attack cone of the predator showed reductions in speed and acceleration characteristic of predator-inspection behaviour. However, when predators became active, prey swam faster with greater acceleration and were closer together within the attack cone of predators. Most importantly, this study provides evidence that prey do not adopt a uniform response to the presence of a predator. Instead, we demonstrate that prey are capable of rapidly and dynamically updating their assessment of risk and showing fine-scale adjustments to their behaviour. },
	Author = {Kent, Maud I. A. and Herbert-Read, James E. and McDonald, Gordon and Wood, A. Jamie and Ward, Ashley J. W.},
	Date-Added = {2020-06-28 17:07:43 +1000},
	Date-Modified = {2020-06-28 17:07:55 +1000},
	Doi = {10.1098/rspb.2019.0448},
	Eprint = {https://royalsocietypublishing.org/doi/pdf/10.1098/rspb.2019.0448},
	Journal = {Proceedings of the Royal Society B: Biological Sciences},
	Number = {1903},
	Pages = {20190448},
	Title = {Fine-scale behavioural adjustments of prey on a continuum of risk},
	Url = {https://royalsocietypublishing.org/doi/abs/10.1098/rspb.2019.0448},
	Volume = {286},
	Year = {2019},
	Bdsk-Url-1 = {https://royalsocietypublishing.org/doi/abs/10.1098/rspb.2019.0448},
	Bdsk-Url-2 = {https://doi.org/10.1098/rspb.2019.0448}}

@article{t3,
	Abstract = {Atom interferometers have been used to measure acceleration with at best a T2 scaling in sensitivity as the interferometer time T is increased. This limits the sensitivity to acceleration which is theoretically achievable by these configurations for a given frequency of acceleration. We predict and experimentally measure the acceleration-sensitive phase shift of a large-momentum-transfer atom interferometer based upon Bloch oscillations. Using this novel interferometric scheme we demonstrate an improved scaling of sensitivity which will scale as T3. This enhanced scaling will allow an increase in achievable sensitivity for any given frequency of an oscillatory acceleration signal, which will be of particular use for inertial and navigational sensors, and proposed gravitational wave detectors. A straightforward extension should allow a T4 scaling in acceleration sensitivity.},
	Author = {G. D. McDonald and C. C. N. Kuhn and S. Bennetts and J. E. Debs and K. S. Hardman and J. D. Close and N. P. Robins},
	Date-Added = {2020-06-28 17:06:19 +1000},
	Date-Modified = {2020-06-28 17:06:27 +1000},
	Doi = {10.1209/0295-5075/105/63001},
	Journal = {{EPL} (Europhysics Letters)},
	Month = {mar},
	Number = {6},
	Pages = {63001},
	Publisher = {{IOP} Publishing},
	Title = {A faster scaling in acceleration-sensitive atom interferometers},
	Url = {https://doi.org/10.1209%2F0295-5075%2F105%2F63001},
	Volume = {105},
	Year = 2014,
	Bdsk-Url-1 = {https://doi.org/10.1209%2F0295-5075%2F105%2F63001},
