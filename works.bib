	Journal = {Proceedings of the International Quantum Electronics Conference and Conference on Lasers and Electro-Optics Pacific Rim 2011},
	Keywords = {Dephasing; Gravity; Laser sources; Optical measurement; Phase shift; Precision metrology},
	Pages = {I1095},
	Publisher = {Optical Society of America},
	Title = {Comparing Thermal and Lasing Atomic Sources for Precision Inertial Measurement},
	Url = {http://www.osapublishing.org/abstract.cfm?URI=IQEC-2011-I1095},
