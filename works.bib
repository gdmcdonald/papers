	Month = {May},
	Number = {9},
	Pages = {10642--10654},
	Publisher = {OSA},
	Title = {External cavity diode lasers with 5kHz linewidth and 200nm tuning range at 1.55$\mu$m and methods for linewidth measurement},
	Url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-22-9-10642},
	Volume = {22},
